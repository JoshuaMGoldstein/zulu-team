 {
        "family": "zulu-team",
        "networkMode": "awsvpc",
        "cpu": "256",
        "memory": "512",
        "executionRoleArn": "arn:aws:iam::477961709976:user/zulu-team-ecs-user:role/ecsTaskExecutionRole",
        "containerDefinitions": [
            {
                "name": "zulu-team-gemini-docker-container",
                "image": "477961709976.dkr.ecr.us-east-1.amazonaws.com/gemini-docker:latest",
                "portMappings": [
                    {
                        "containerPort": 80,
                        "hostPort": 80,
                        "protocol": "tcp"
                    }
                ],
                "essential": true
            }
        ],
        "requiresCompatibilities": ["FARGATE"]
    }
